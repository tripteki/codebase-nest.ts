{
    "name": "codebase",
    "version": "1.0.1",
    "private": true,
    "description": "Project",
    "license": "UNLICENSED",

    "strict": true,
    "engines": { "node": ">=20.15.0" },

    "scripts": {

        "doc": "bunx @compodoc/compodoc -p tsconfig.json -d public/documentation --hideDarkModeToggle --hideGenerator --disableDependencies --disableSourceCode --disableTemplateTab --disableStyleTab",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "bun run format && eslint \"{src,test}/**/*.ts\" --fix",
        "prisma": "bun run postgre && bun run mongo",
        "postgre": "bunx prisma generate --schema=./src/app/databases/postgre.schema.prisma",
        "postgre:migrate": "bunx prisma migrate dev --schema=./src/app/databases/postgre.schema.prisma",
        "postgre:status": "bunx prisma migrate status --schema=./src/app/databases/postgre.schema.prisma",
        "mongo": "bunx prisma generate --schema=./src/app/databases/mongo.schema.prisma",
        "mongo:migrate": "bunx prisma db push --schema=./src/app/databases/mongo.schema.prisma",
        "build": "nest build",
        "start": "nest start",
        "repl": "bun run start -- --watch --entryFile repl",
        "cli": "bun run build && bun ./dist/cli.js",
        "dev": "NODE_ENV=local nest start --debug --watch",
        "prod": "bun run build && NODE_ENV=production bun ./dist/main",
        "test": "bun run test:e2e --watch",
        "test:e2e": "jest --config test/jest-e2e.json --testRegex='\\.e2e-spec\\.ts$' --passWithNoTests --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",

        "postinstall": "bun run prisma"
    },

